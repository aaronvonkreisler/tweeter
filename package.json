{
  "name": "tweeter",
  "version": "1.0.0",
  "description": "MERN Stack Twitter Clone",
  "main": "dist/index.js",
  "engines": {
    "node": "12.18.3",
    "yarn": "1.22.10",
    "npm": "6.14.10"
  },
  "scripts": {
    "start": "node dist/index.js",
    "server": "nodemon server",
    "dev": "nodemon --exec yarn restart",
